/**
 * Да се напише програма со која ќе се пребројат колку четирицифрени броеви
 * постојат за кои сумата од последните две цифри е 4. Пример за такви 
 * четирицифрени броеви: 1004, 1013, ..., 1104, 1113, 11122... 
 */
#include <iostream>
using namespace std;
int main()
{
    int i, k=0, e, d;

    for (i=1000;i<10000;i++){
        e = i%10;
        d = i%100%10;
        if (e+d==4)
            k++;
    }
    cout<<"Vkupno ima " <<k<<" broevi";
    return 0;
    }
