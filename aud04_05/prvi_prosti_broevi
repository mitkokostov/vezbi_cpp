/**
 * Програма која ќе ги испише првите n прости броеви
 */

#include <iostream>
using namespace std;
int main()
{
    int i, n, k, N;
    bool Prost = true;
    cout << "Vnesi broj: ";
    cin >> N;
    n = 2;
    k = 0;
    while (k < N) {
            for (i = 2; i <= n/2; ++i) {
                if (n%i == 0) {Prost = false; break;}
    }
    if (Prost) {cout << n << endl; ++k;}
    ++n;
    Prost = true;
    }
    return 0;
}
