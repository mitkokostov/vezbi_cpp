/** 
  * Да се напише програма со која се внесуваат 15 цели броја 
  * и се собираат само двоцифрените
  */
#include <iostream>

using namespace std;

int main()
{
    int i, x, s=0;
    cout<<"Vnesi 15 broevi:\n";
    for (i=1; i<=15; i++){
        cin>>x;
        if (x<100 && x>9)
        s=s+x;
    }
    cout<<endl<<"Zbirot na dvocifrenite broevi e: "<<s;

    return 0;
}
